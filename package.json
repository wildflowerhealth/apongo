{
  "name": "@wildflowerhealth/apongo",
  "version": "4.16.0",
  "description": "Create Mongo aggregation pipelines with recursive joins for Apollo queries.",
  "main": "dist/index.js",
  "keywords": [
    "mongo",
    "apollo",
    "join",
    "aggregation"
  ],
  "author": "Timothy Armes <tim@timothyarmes.com> (http://www.timothyarmes.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/wildflowerhealth/apongo"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "version": "npm run clean && npm run build && git add dist",
    "postversion": "git push && git push --tags",
    "pretty": "prettier -w .",
    "clean": "rm -rf dist",
    "build": "tsc",
    "watch": "tsc --watch --preserveWatchOutput",
    "precommit": "npm run build && git add dist",
    "deploy": "npm publish",
    "test": "jest",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "@graphql-tools/utils": "^8.1.1",
    "apollo-server-core": "^3.0.2",
    "graphql": "^15.7.2",
    "graphql-tag": "^2.9.2",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@graphql-tools/schema": "^8.1.2",
    "apollo-server": "^3.1.2",
    "apollo-server-core": "^3.1.2",
    "babel-jest": "^26.6.3",
    "graphql": "^15.7.2",
    "graphql-tag": "^2.12.5",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^12.1.2",
    "merge-graphql-schemas": "^1.7.8",
    "mongo-unit": "^1.5.1",
    "mongodb-prebuilt": "^6.5.0",
    "mongoose": "^5.13.7",
    "prettier": "^2.5.0",
    "type-graphql": "^1.1.1",
    "typescript": "^4.5.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@wildflowerhealth"
  }
